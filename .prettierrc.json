{
  "printWidth": 120,
  "tabWidth": 2,
  "useTabs": false,
  "semi": false,
  "quoteProps": "as-needed",
  "trailingComma": "es5",
  "bracketSpacing": false,
  "arrowParens": "always",
  "singleQuote": true,
  "importOrder": ["^[^.]", "^[.]"],
  "importOrderSeparation": false,
  "importOrderSortSpecifiers": true
}

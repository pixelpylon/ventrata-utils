{
  "name": "@exp1/ventrata-products-utils",
  "version": "0.0.1",
  "description": "Ventrata products utils",
  "typings": "dist/declares/index.d.ts",
  "scripts": {
    "lint": "eslint --fix --ext .js .",
    "test": "mocha 'test/**/*.test.js'",
    "test:dev": "mocha 'test/**/*.test.js' --watch",
    "prepare": "husky install",
    "artifactregistry-login": "npx google-artifactregistry-auth",
    "publish-package": "bash -c 'publish_pkg () { npm run artifactregistry-login && npm publish && git add -A && git ci -m \"${0:=$(npm pkg get version)}\" && git tag $(npm pkg get version) && git push && git push --tags; }; publish_pkg'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/citytourdev/ventrata-products-utils.git"
  },
  "engines": {
    "node": ">=12"
  },
  "author": "Kosolapov Ivan <polzuka@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.0.0",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "chai": "^4.3.6",
    "eslint": "^8.1.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-require-path-exists": "^1.1.9",
    "husky": "^8.0.0",
    "lint-staged": "^13.1.0",
    "mocha": "^10.0.0",
    "prettier": "2.8.0",
    "@exp1/ventrata-utils": "^0.0.99"
  },
  "peerDependencies": {
    "@exp1/ventrata-utils": ">=0.0.99"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  }
}

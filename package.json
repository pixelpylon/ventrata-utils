{
  "name": "@exp1/ventrata-utils",
  "version": "1.0.0",
  "description": "Ventrata utils",
  "main": "dist/src/index.js",
  "types": "dist/types/index.d.ts",
  "scripts": {
    "lint": "eslint --fix .",
    "test": "mocha 'test/**/*.test.js'",
    "test:dev": "mocha 'test/**/*.test.js' --watch",
    "prepare": "husky install",
    "artifactregistry-login": "npx google-artifactregistry-auth",
    "publish-package": "bash -c 'publish_pkg () { npm run artifactregistry-login && npm publish && git add -A && git ci -m \"${0:=$(npm pkg get version)}\" && git tag $(npm pkg get version) && git push && git push --tags; }; publish_pkg'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pixelpylon/ventrata-utils.git"
  },
  "engines": {
    "node": ">=20"
  },
  "author": "Kosolapov Ivan <polzuka@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "eslint": "^8.56.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-import": "^2.29.1",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "chai": "^4.3.6",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "mocha": "^10.4.0",
    "prettier": "3.2.5",
    "@exp1/back-utils": "^1.0.0",
    "@exp1/common-utils": "^1.0.0",
    "lodash": "^4.17.21",
    "moment": "^2.30.1"
  },
  "peerDependencies": {
    "@exp1/common-utils": ">=1.0.0",
    "@exp1/back-utils": ">=1.0.0",
    "lodash": ">=4.17.21",
    "moment": ">=2.30.1"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  }
}
